<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_register4_almost_finished"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:background="@color/colorPrimary"
    tools:context="swarm_app_3.ehb.com.contourtheapp.Activities.Register4AlmostFinished">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/hoogtelijnenbackground"
        android:id="@+id/imageView2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="@string/finishtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:id="@+id/finishtitle"
        android:textColor="@color/colorPrimaryDark"
        android:layout_marginStart="25dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textSize="18dp" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="20dp"
        app:srcCompat="@drawable/inschrijving4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cometoitaly"
        android:textColor="@color/colorPrimaryDark"
        android:layout_marginLeft="5dp"
        android:text="@string/cometoitaly"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toEndOf="@+id/textView7" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_height="30dp"
        android:background="@android:drawable/btn_dropdown"
        android:spinnerMode="dropdown"
        android:layout_width="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:layout_marginTop="5dp" />

    <TextView
        android:text="@string/prev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblPrev"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:background="@android:color/white"
        android:onClick="goToRegi3"
        android:paddingLeft="15dp"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:paddingRight="15dp"
        android:layout_marginBottom="26dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblfinish"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:background="@android:color/white"
        android:onClick="goToHome"
        android:paddingLeft="15dp"
        android:paddingBottom="5dp"
        android:paddingTop="5dp"
        android:paddingRight="15dp"
        android:layout_marginEnd="11dp"
        android:layout_alignBaseline="@+id/lblPrev"
        android:layout_alignBottom="@+id/lblPrev"
        android:layout_alignParentEnd="true"
        android:text="@string/finish" />

    <TextView
        android:text="|"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:textColor="@color/colorAccent"
        android:textSize="20sp"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/finishtitle"
        android:layout_alignStart="@+id/finishtitle" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/howmanypeople"
        android:textColor="@color/colorPrimaryDark"
        android:layout_marginLeft="5dp"
        android:text="@string/howmanypeople"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toEndOf="@+id/textView8" />

    <TextView
        android:text="|"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:textColor="@color/colorAccent"
        android:textSize="20sp"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/spinner1"
        android:layout_alignStart="@+id/spinner1" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="15"
        android:id="@+id/editText5"
        android:background="@color/lightgrey"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:layout_below="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:layout_marginTop="5dp"
        android:textColor="?attr/colorPrimaryDark"
        android:paddingLeft="5dp" />

</RelativeLayout>
